{
  "name": "transactions-management",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "npm run typeorm -- migration:create ./src/infrastructure/database/migrations/InitMigration",
    "migration:generate": "npm run typeorm -- migration:generate ./src/infrastructure/database/migrations/InitMigration -d ./src/infrastructure/database/config/typeorm.config.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/infrastructure/database/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/infrastructure/database/config/typeorm.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.10.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "inversify": "^6.1.5",
    "mysql2": "^3.11.5",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  }
}
